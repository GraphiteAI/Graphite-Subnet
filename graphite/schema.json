                    ],
                    "title": "nonce"
                },
                "uuid": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "A unique identifier associated with the terminal",
